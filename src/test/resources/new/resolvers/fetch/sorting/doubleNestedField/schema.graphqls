type Query {
  query(orderBy: Order @orderBy, first: Int = 100, after: String): CustomerConnection
}

enum OrderByField {
  NAME @index(name : "IDX_LAST_NAME")
}

type Customer @table {
  nested: Nested1
}

type Nested1 {
  nested: Nested2
}

type Nested2 {
  name: String @field(name: "LAST_NAME")
}
