type Query {
  query(order: Order @orderBy, first: Int = 100, after: String): InventoryConnection
}

type InventoryConnection {
  edges: [InventoryConnectionEdge]
  pageInfo: PageInfo
  nodes: [Inventory!]!
}

type InventoryConnectionEdge {
  cursor: String
  node: Inventory
}

type Inventory @table {
  storeId: String! @field(name: "STORE_ID")
  filmId: String! @field(name: "FILM_ID")
}

enum OrderByField {
  STORE_ID_FILM_ID @index(name : "IDX_STORE_ID_FILM_ID")
}
