type Query {
  query: Address @service(service: {name: "DUMMY_SERVICE"})
}

type Address @record(record: {name: "MAPPER_RECORD_ADDRESS"}) {
  cityJOOQ: AddressCityJOOQ
  cityJava: AddressCityJava
}

type AddressCityJOOQ @table(name: "City") {
  city: String
}

type AddressCityJava @record(record: {name: "MAPPER_RECORD_CITY"}) {
  cityName: String
}
