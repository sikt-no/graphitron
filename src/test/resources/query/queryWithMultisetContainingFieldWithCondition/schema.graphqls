type Film implements Node @table {
  id: ID!
  title: String!
  mainActor: FilmActor! @reference(condition: {name: "TEST_FILM_ACTOR", method: "film_filmActor"})
}

type FilmActor implements Node @table(name: "FILM_ACTOR") {
  id: ID!
}

type Inventory implements Node @table {
  id: ID!
  films: [Film!]!
}

type Rental implements Node @table {
  id: ID!
  inventory: Inventory! @splitQuery
}

type Query {
  inventory: [Rental!]!
}
