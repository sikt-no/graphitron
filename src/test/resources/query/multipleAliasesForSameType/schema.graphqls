type Query {
  film: Film
}

type Film @table {
  id: ID!
  filmLanguage: Language @reference(references: [{key: "FILM__FILM_LANGUAGE_ID_FKEY"}]) #reference 1 for film_language
  originalLanguage: Language @reference(references: [{key: "FILM__FILM_ORIGINAL_LANGUAGE_ID_FKEY"}]) #reference 2 for film_language
}

type Language @table {
  id: ID!
}
