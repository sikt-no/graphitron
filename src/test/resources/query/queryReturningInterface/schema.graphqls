type Film implements Titled & Node @table {
  id: ID!
  title: String!
}

type Inventory implements Node @table {
  id: ID!
  film: Film!
  rentals: [Rental!]! @splitQuery
}

type Query {
  node(id: ID!): Node
  titled(title: String!): Titled #A bit weird example, since multiple things/films can have the same title
  nodes(ids: [ID!]): [Node] @notGenerated
}

type Rental implements Node @table {
  id: ID!
}

interface Titled {
  title: String!
}
