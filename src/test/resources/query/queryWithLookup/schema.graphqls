type Query {
  customers(storeIds: [ID!]! @field(name: "STORE_ID") @lookupKey): [Customer] # ID key
  countries(countryNames: [String!]! @field(name: "COUNTRY") @lookupKey): [Country] # Other key
}

type Customer implements Node @table {
  id: ID!
}

type Country implements Node @table {
  id: ID!
}
