type Query {
  person(
    eierInstitusjonsnummer: String! @column(name: "INSTITUSJONSNR_EIER"),
    first: Int = 100,
    after: String,
  ): PersonProfilConnection @connection(for: "PersonProfil")

  person2(
    eierInstitusjonsnummer: String! @column(name: "INSTITUSJONSNR_EIER"),
    first: Int = 100,
    after: String,
  ): PersonProfilConnection @connection(for: "PersonProfil")
}

type PersonProfil @table(name: "PERSON")  {
  id: ID!
  fornavn: String!
}
