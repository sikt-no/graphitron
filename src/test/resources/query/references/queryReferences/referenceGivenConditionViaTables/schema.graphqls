type Query {
  rental: Rental @notGenerated
}

type Rental @table {
  id: ID!
  # join path: RENTAL -> INVENTORY -> FILM -> FILM_ACTOR(via condition)
  mainActor: FilmActor! @splitQuery @reference(
    via: [{table: "INVENTORY"}, {table: "FILM"}],
    condition: {name: "TEST_FILM_ACTOR", method: "film_filmActor"})
}

type FilmActor @table(name : "FILM_ACTOR") {
  id: ID!
  actor: Actor!
}

type Actor @table {
  id: ID!
  lastName: String! @field(name: "LAST_NAME")
}
