type Query {
  inventory: Inventory @notGenerated
}

type Inventory @table {
  id: ID!
  # join path: INVENTORY -> FILM -> FILM_ACTOR (via condition) -> ACTOR
  mainActors: [Actor!]! @splitQuery @reference(references: [{table: "FILM"}, {table: "FILM_ACTOR", condition: {name: "TEST_FILM_ACTOR", method: "film_filmActor"}}])
}

type Actor @table {
  id: ID!
  lastName: String! @field(name: "LAST_NAME")
}
