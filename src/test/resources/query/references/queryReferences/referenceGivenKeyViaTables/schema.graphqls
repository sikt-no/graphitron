type Query {
  payment: Payment @notGenerated
}

type Payment @table {
  id: ID!
  #join path: PAYMENT -> RENTAL -> INVENTORY -> FILM -> LANGUAGE(via key)
  originalLanguage: Language! @splitQuery @reference(
    references: [{table: "RENTAL"}, {table: "INVENTORY"}, {table: "FILM"}, {key: "FILM__FILM_ORIGINAL_LANGUAGE_ID_FKEY"}])
}

type Language @table {
  id: ID!
  name: String!
}