type Query {
  store(
    id: ID!,
    #join path: STORE -> CUSTOMER (via condition) -> ADDRESS (via key) -> CITY (via key)
    name: String! @reference(
      references: [{table: "CUSTOMER", condition: {name: "TEST_STORE_CUSTOMER", method: "storeCustomer"}}, {key: "CUSTOMER__CUSTOMER_ADDRESS_ID_FKEY"}, {key: "ADDRESS__ADDRESS_CITY_ID_FKEY"}],
    )
  ): Store
}

type Store @table {
  id: ID!
}
