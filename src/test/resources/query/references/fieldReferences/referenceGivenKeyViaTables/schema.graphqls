type Query {
  payment(id: ID!): Payment
}

type Payment @table {
  id: ID!
  #join path: PAYMENT -> RENTAL -> INVENTORY -> FILM -> LANGUAGE(via key)
  originalLanguageName: String! @field(name: "NAME") @reference(
    via: [{table: "RENTAL"}, {table: "INVENTORY"}, {table: "FILM"}],
    table: "LANGUAGE",
    key: "FILM__FILM_ORIGINAL_LANGUAGE_ID_FKEY"
  )
}
