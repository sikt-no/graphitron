type Query {
  programStudierett(
    id: String!
  ): ProgramStudierett
}

type ProgramStudierett @node(table: "STUDIERETT") {
  id: ID!
  studieretning: Studieretning @splitQuery
  personProfil: PersonProfil @splitQuery
}

type Studieretning @node {
  id: ID!
  navn: StudieretningnavnWithOptional!   #required, with optional "sub fields"
  navnOpt: StudieretningnavnWithOptional #optional, with optional "sub fields"
  navnReq: StudieretningnavnAllRequired! #required, with all "sub fields required"
}

type StudieretningnavnWithOptional {
  eng: String @column(name: "STUDIERETNINGNAVN_ENGELSK")
  nob: String! @column(name: "STUDIERETNINGNAVN_BOKMAL")
  nno: String @column(name: "STUDIERETNINGNAVN_NYNORSK")
  nob2: String! @column(name: "STUDIERETNINGNAVN_BOKMAL")
}

type StudieretningnavnAllRequired {
  nob: String! @column(name: "STUDIERETNINGNAVN_BOKMAL")
  nob2: String! @column(name: "STUDIERETNINGNAVN_BOKMAL")
}

