
# Skjema for å teste at påkrevde felter IKKE kan bli null. "nullOnAllNull" legges ikke på i mapping av
# påkrevde typer som inneholder ikke-påkrevde felter. I mappingen på nivået utenfor, legges det på en forsterket
# nullOnAllNull-mapping som i tillegg til null også behandler "tomme objekter" som null.

type Query {
  inventory(
    id: String! @field(name: "INVENTORY_ID")
  ): Inventory
}

type Inventory @table {
  id: ID!
  store: Store @splitQuery
  film: Film @splitQuery
}

type Store @table {
  id: ID!
  fields: StoreFieldsWithOptional!   #required, with optional "sub fields"
  navnOpt: StoreFieldsWithOptional #optional, with optional "sub fields"
  navnReq: StoreFieldsAllRequired! #required, with all "sub fields required"
}

type StoreFieldsWithOptional {
  filmId: Int! @field(name: "FILM_ID")
  lastUpdate: DateTime @field(name: "LAST_UPDATE")
}

type StoreFieldsAllRequired {
  filmId: Int! @field(name: "FILM_ID")
  addressId: Int! @field(name: "ADDRESS_ID")
}

