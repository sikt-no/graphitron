  forsteTerminForVurdering: Termin! @splitQuery @reference(
    via: [{table: "VURDERINGSENHET"}, {table: "VURDERINGSKOMBINASJON"}, {table: "EMNE"}],
    key: "EMNE__EKS_FORSTE__TERMIN__FK")
}

type Termin @table {
