type Mutation {
  mutation: Response! @mutation(typeName: UPDATE)
}

type Response {
  errors: [OtherError!]
}

type OtherError implements Error @error(handlers:
[{handler: GENERIC, className: "org.example.exception.ThereIsNoSuchException", description: "This is an error"}]) {
  path: [String!]!
  message: String!
}
