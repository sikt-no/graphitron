type Mutation {
  m(i: [Input!]!): ID! @mutation(typeName: UPSERT)
}

input Input @table(name: "FILM") {
  id: ID! @field(name: "FILM_ID")
  title: String!
  languageId: Int! @field(name: "LANGUAGE_ID")
  fulltext: String!
  duration: Int @field(name: "RENTAL_DURATION")
  rating: Rating
}

enum Rating {
  G @field(name: "G"),
  PG_13 @field(name: "PG-13"),
  R @field(name: "R"),
}
