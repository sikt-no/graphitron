input InsertCustomerInput @table(name : "CUSTOMER") {
  customerID: String! @column(name : "CUSTOMER_ID")
  active: Boolean! @column(name: "ACTIVEBOOL")
  firstName: String @column(name : "FIRST_NAME") #should not be optional
  lastName: String! @column(name : "LAST_NAME")
  storeID: String! @column(name : "STORE_ID")
  addressID: String! @column(name : "ADDRESS_ID")
  createdDate: Date @column(name : "CREATE_DATE")  #should not be optional
}

type Mutation {
  registerCustomerInput(input: InsertCustomerInput!): ID! @mutationType(typeName: INSERT)
}
