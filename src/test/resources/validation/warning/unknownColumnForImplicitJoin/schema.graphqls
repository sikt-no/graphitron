type Query {
  customer(id: ID!): Customer
}

type Customer @table {
  id: ID!
  address: CustomerAddress!
}

type CustomerAddress {
  district: String @field(name: "district") @reference(table: "ADDRESS") #should NOT trigger warning
  destruct: String @field(name: "destruct") @reference(table: "ADDRESS") #non-existent column, should trigger warning
}
