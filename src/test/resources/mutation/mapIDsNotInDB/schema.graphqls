type Mutation {
  editCustomerAddress(in: EditAddressInput!): EditAddressResponse! @service(service: {name:"TEST_CUSTOMER"})
}

input EditAddressInput @table(name: "CUSTOMER") {
  id: ID!
  addressId: ID! @field(name: "ADDRESS_ID")
}

type EditAddressResponse @record(record: {name: "TEST_CUSTOMER_ADDRESS_RESPONSE"}) {
  id: ID!
  addressId: ID!
}
