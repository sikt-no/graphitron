<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <mirrors>
        <mirror>
            <id>jboss-public-repository-group-https</id>
            <mirrorOf>jboss-public-repository-group</mirrorOf>
            <name>Jboss public https</name>
            <url>https://repository.jboss.org/nexus/content/groups/public/</url>
        </mirror>
    </mirrors>
    <servers>
        <server>
            <username>${env.ARTIFACTORY_USERNAME}</username>
            <password>${env.ARTIFACTORY_PASSWORD}</password>
            <id>artifactory-central-libs</id>
        </server>
        <server>
            <username>${env.ARTIFACTORY_USERNAME}</username>
            <password>${env.ARTIFACTORY_PASSWORD}</password>
            <id>artifactory-central-thirdparty</id>
        </server>
        <server>
            <id>artifactory-central</id>
            <username>${env.ARTIFACTORY_USERNAME}</username>
            <password>${env.ARTIFACTORY_PASSWORD}</password>
        </server>
        <server>
            <id>artifactory-libs-snapshots</id>
            <username>${env.ARTIFACTORY_USERNAME}</username>
            <password>${env.ARTIFACTORY_PASSWORD}</password>
        </server>
        <server>
            <id>artifactory-libs-releases</id>
            <username>${env.ARTIFACTORY_USERNAME}</username>
            <password>${env.ARTIFACTORY_PASSWORD}</password>
        </server>
        <server>
            <id>artifactory-libs</id>
            <username>${env.ARTIFACTORY_USERNAME}</username>
            <password>${env.ARTIFACTORY_PASSWORD}</password>
        </server>
    </servers>
    <profiles>
        <profile>
            <repositories>
                <repository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>artifactory-libs-release</id>
                    <name>libs-releases</name>
                    <url>https://artifactory.sikt.no/artifactory/libs-releases</url>
                </repository>
                <repository>
                    <id>artifactory-libs-snapshots</id>
                    <name>libs-snapshots</name>
                    <url>https://artifactory.sikt.no/artifactory/libs-snapshots</url>
                    <releases>
                        <enabled>false</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </repository>
                <repository>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                    <id>artifactory-central</id>
                    <name>libs-releases</name>
                    <url>https://artifactory.sikt.no/artifactory/libs-releases</url>
                </repository>
                <repository>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                    <id>artifactory-central-thirdparty</id>
                    <name>third-party</name>
                    <url>https://artifactory.sikt.no/artifactory/third-party</url>
                </repository>

                <repository>
                    <id>central</id>
                    <name>Central Repository</name>
                    <url>https://repo.maven.apache.org/maven2</url>
                    <layout>default</layout>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                    <id>jboss-thirdparty</id>
                    <name>jboss-third-party</name>
                    <url>https://repository.jboss.org/nexus/content/repositories/thirdparty-releases/</url>
                </repository>
            </repositories>
            <id>artifactory-unit</id>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>artifactory-unit</activeProfile>
    </activeProfiles>
</settings>
