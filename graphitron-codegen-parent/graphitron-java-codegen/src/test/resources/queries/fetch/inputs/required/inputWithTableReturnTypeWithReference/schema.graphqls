type Query {
  cityByName(in: CityNameInput!): City
}

type City @table {
    id: ID!
    name: String! @field(name: "CITY_")
    countryName: String! @field(name: "COUNTRY_") @reference(path: [{table: "COUNTRY"}])
}

input CityNameInput @table(name: "CITY") {
    name: String! @field(name: "CITY_")
}

