type Query {
    customer(first_name: String): Customer @tableMethod(tableMethodReference: {className: "no.sikt.graphitron.codereferences.services.CustomerTableMethod", method: "customerTable"})
}

type Customer @table{
    id: ID
    staff: Staff @reference(path: [{table: "Address"}]) @tableMethod(tableMethodReference: {className: "no.sikt.graphitron.codereferences.services.StaffTableMethod", method: "staffTable"})
}

type Staff @table{
    id: ID
}