type Query {
  address: Address
}

interface Address @table(name: "ADDRESS") @discriminate(on: "DISTRICT"){
  postalCode: String @field(name: "POSTAL_CODE")
}

type AddressInDistrictOne implements Node & Address @table(name: "ADDRESS") @discriminator(value: "ONE") @node(typeId: "A1", keyColumns: "ADDRESS_ID"){
  id: ID!
  postalCode: String @field(name: "POSTAL_CODE")
}

type AddressInDistrictTwo implements Node & Address @table(name: "ADDRESS") @discriminator(value: "TWO") @node(typeId: "A2"){
  id: ID!
  postalCode: String @field(name: "POSTAL_CODE")
}
