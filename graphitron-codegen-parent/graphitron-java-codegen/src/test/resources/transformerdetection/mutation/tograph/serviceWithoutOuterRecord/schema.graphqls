type Mutation {
  edit1: Response1! @service(service: {name: "DUMMY_SERVICE"})
  edit2: Response2! @service(service: {name: "DUMMY_SERVICE"})
}

type Response1 {
  customer1: [Customer1]
}

type Response2 {
  customer2: [Customer2] @splitQuery
}

type Customer1 @table(name: "CUSTOMER") {
  id: ID!
}

type Customer2 @table(name: "CUSTOMER") {
  id: ID!
}
